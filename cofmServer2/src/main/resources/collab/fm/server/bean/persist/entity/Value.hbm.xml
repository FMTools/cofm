<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="collab.fm.server.bean.persist.entity">
  <joined-subclass name="Value" extends="collab.fm.server.bean.persist.Element" table="VALUE">
    <key column="VALUE_ID" />
    
    <property name="val" type="string" column="VALUE_STRING" />
    
  </joined-subclass>
</hibernate-mapping>