<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="quiz.server.bean">
  <class name="Candidate" table="CANDIDATE">
  
  	<id name="id" type="integer" column="CANDIDATE_ID">
  		<generator class="native"></generator>
  	</id>
  	
  	<many-to-one name="quiz" class="Quiz" column="QUIZ_ID" not-null="true"></many-to-one>
  	
  	<property name="content" type="string" column="CONTENT"></property>
  	<property name="correct" type="boolean" column="IS_CORRECT"></property>
  </class>
</hibernate-mapping>