<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="quiz.server.bean">
  <class name="Answer" table="ANSWER">
  	
  	<id name="id" type="integer" column="ANSWER_ID">
  		<generator class="native"></generator>
  	</id>
  	
  	<property name="time" type="long" column="TIME"></property>
  	<property name="quizNo" type="integer" column="QUIZ_NO"></property>
  	<property name="choice" type="string" column="CHOICE"></property>
  	<property name="score" type="integer" column="SCORE"></property>
  	<many-to-one name="user" class="User" column="USER_ID"></many-to-one>
  </class>
</hibernate-mapping>